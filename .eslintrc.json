{
  "plugins": [
    "prettier"
  ],
  "extends": [
    "prettier",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "settings": {
    "react": {
      "version": "16.x"
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "no-unused-vars": "error",
    "eqeqeq": "error",
    "curly": "error"
  }
}
