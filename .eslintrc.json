{
  "plugins": ["prettier"],
  "extends": [
    "prettier",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "no-unused-vars": "error",
    "eqeqeq": "error",
    "curly": "error"
  }
}
